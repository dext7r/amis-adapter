{
  "version": 2,
  "env": {
    "RUNNING_ENV": "prod"
  },
  "cleanUrls": true,
  "builds": [
    {
      "src": "./dist/**",
      "use": "@vercel/static"
  }
  ],
  "outputDirectory": "dist",
  "public": true,
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/$1"
    },
    {
      "source": "/amis-editor-webpack",
      "destination": "https://amis-editor-webpack.h7ml.cn/"
    }
  ]
}
