    "@rspack/cli": "0.6.3",
    "@rspack/core": "0.6.3",
    "@rspack/plugin-react-refresh": "0.6.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.1.1",
